{
  "name": "Katapulta",
  "version": "1.0.0",
  "description": "Pour emprunter ou preter son argent",
  "main": "src/server.js",
  "scripts": {
    "lint": "eslint src",
    "start-dev": "NODE_ENV=production node_modules/.bin/babel-node --presets 'react,es2015' src/server.js",
    "start-dev-ui": "npm run start-dev-hmr",
    "start-dev-hmr": "node_modules/.bin/webpack-dev-server --progress --inline --hot",
    "auto-start": "NODE_ENV=development nodemon --exec \"npm run lint && npm start-dev\" --ignore public/js",
    "build": "NODE_ENV=production node_modules/.bin/webpack -p",
    "clean": "rm -rf ./dist/*",
    "copy:ejs": "cp -rf src/views ./dist/views",
    "copy:static": "cp -rf src/static/ ./dist/static",
    "copy": "npm run copy:ejs && npm run copy:static",
    "compile-prod": "npm run clean && npm run copy && babel src -d dist --presets=react,es2015,stage-2",
    "start-prod": "NODE_ENV=production node dist/server.js",
    "deploy": "NODE_ENV=production webpack -p --config webpack.config.production.js --progress"
  },
  "author": "Julien Fumanti",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.18.0",
    "ejs": "^2.5.1",
    "express": "^4.14.0",
    "flux": "^2.1.1",
    "jquery": "~2.1.4",
    "material-ui": "^0.16.1",
    "react": "^15.3.1",
    "react-bootstrap": "^0.30.1",
    "react-dom": "^15.3.1",
    "react-responsive-carousel": "^3.0.21",
    "react-router": "^2.6.1",
    "react-scrollchor": "^2.1.4",
    "toastr": "^2.1.2"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.5.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "keymirror": "^0.1.1",
    "less-loader": "^2.2.3",
    "nodemon": "^1.11.0",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.14.1"
  }
}
